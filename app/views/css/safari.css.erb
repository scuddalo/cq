a:hover, a:focus, div#content a:hover, div#content a:focus {
	cursor: pointer;
}

div#header {
	-webkit-border-radius-top-left: 18px;
	-webkit-border-radius-top-right: 18px;
}

div#navbar {
	-webkit-border-radius-bottom-left: 18px;
	-webkit-border-radius-bottom-right: 18px;
}

form input.button, form input[type="button"], form input.submit, form input[type="submit"] {
	cursor: pointer;
}
